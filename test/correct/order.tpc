/* test for pointers */
/* int tab(int *x, int *y); */

int main(void) {
	int a;
	a = 0;
	
	while (a < 4){
		print(a);
		a = a + 1;	
	}

	a = 0;

	while(a <= 4){
		print(a);
		a = a + 1;
	}

	a = 4;

	while(a > 0){
		print(a);
		a = a - 1;
	}

	a = 4;

	while(a >= 0){
		print(a);
		a = a - 1;
	}			

}


